{
  "name": "You're on Production!",
  "short_name": "Y.O.P.!",
  "description": "An extension that warns you when you are on production!",
  "version": "1.0.0",
  "author": "Aubrey Portwood",
  "background": {
     "scripts": [
        "background.js"
      ],
     "persistent": false
   },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/unknown.png"
    },
    "default_title": "Unknown"
  },
  "manifest_version": 2,
  "offline_enabled": true,
  "minimum_chrome_version": "97.0.4692.99"
}
