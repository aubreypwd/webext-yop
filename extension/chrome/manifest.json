{
  "name": "You're on Production!",
  "description": "Warns you when you are on production, a staging site, or local dev.",
  "version": "1.0",
  "background": {
     "scripts": [
        "background.js"
      ],
     "persistent": false
   },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/unknown.png"
    },
    "default_title": "Unknown"
  },
  "manifest_version": 2
}
